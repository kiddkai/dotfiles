{
  "title": "Left control with delete/backspace to enter",
  "rules": [
    {
      "description": "Left control with delete/backspace to enter",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
